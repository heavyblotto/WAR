        { "name": "badges", "icons": ["tier", "weather", "bounty", "featured"] },
        { "name": "reputationBar", "fill": "{semantic.color.brand.accent}" }
      ]
    },
    {
      "name": "StreakBadge",
      "variants": { "grace": ["available", "consumed"] },
