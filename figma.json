{
  "components": [
    {
      "name": "LeverButton",
      "variants": {
        "state": ["default", "hover", "pressed", "disabled"],
        "size": ["md", "lg"]
      },
      "slots": [
        { "name": "container", "fill": "{semantic.color.controls.leverBg}", "radius": "{semantic.radius.panel}", "shadow": "{semantic.shadow.raised}" },
        { "name": "knob", "fill": "{semantic.color.controls.leverKnob}", "radius": "{semantic.radius.chip}" },
        { "name": "label", "font": "{semantic.font.label}", "color": "{semantic.color.text.primary}" }
      ],
      "motion": {
        "pressIn": { "duration": "{semantic.motion.leverPress.duration}", "easing": "{semantic.motion.leverPress.easing}" },
        "rebound": { "duration": "{semantic.motion.leverRebound.duration}", "easing": "{semantic.motion.leverRebound.easing}" }
      }
    },
    {
      "name": "CardStage",
      "variants": { "territory": ["hearts", "spades", "diamonds", "clubs"] },
      "slots": [
        { "name": "reelArea", "fill": "{semantic.color.bg.card}", "radius": "{semantic.radius.panel}" },
        { "name": "leftDiscard", "opacity": "0.75" },
        { "name": "rightDiscard", "opacity": "0.75" },
        { "name": "warOverlay", "tint": "{semantic.color.territory.$variant}", "mode": "additive" }
      ],
      "motion": {
        "flip": { "duration": "{semantic.motion.flip.duration}", "easing": "{semantic.motion.flip.easing}" },
        "warStack": { "duration": "{semantic.motion.warStack.duration}", "repeat": 3 },
        "warReveal": { "duration": "{semantic.motion.warReveal.duration}" }
      }
    },
    {
      "name": "HealthBar",
      "variants": { "hasArmor": ["true", "false"] },
